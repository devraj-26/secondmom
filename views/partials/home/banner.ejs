<!-- partials/banner.ejs -->
<div id="home-2" class="homepage-slides owl-carousel">
    <% banners.forEach(slide => { %>
      <div class="single-slide-item d-flex align-items-center" data-background="<%= process.env.S3_BASE_URL + slide.bannerImage.webView %>">
        <div class="hero-area-content">
          <div class="container">
            <div class="row align-items-center">
              <div class="col-xl-7 col-lg-8 col-md-10 col-sm-8 wow fadeInUp animated" data-wow-delay=".2s">
                <div class="section-title">
                  <h2 class="text-white"><%= slide.title %></h32>
                </div>
                <p class="text-white"><%= slide.subTitle %></p>
                <!-- <a href="<%= slide.hyperLink %>" class="theme-btn mt-40">Learn More</a> -->
              </div>
            </div>
          </div>
        </div>
      </div>
    <% }) %>
  </div>
  