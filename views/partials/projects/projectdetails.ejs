<script>
    document.addEventListener('DOMContentLoaded', () => {
        const container = document.querySelector('#scrolling-container'); // The container holding the cards
        const cardWrapper = document.querySelector('#card-wrapper'); // The wrapper holding the cards
        const scrollSpeedInMs = 20; // Time in milliseconds for each scroll step
        const scrollDistance = 2;// Pixels to scroll per interval (increase for faster scrolling)

        let scrollInterval;
        let isHovered = false;

        // Function to make the scroll effect continuous
        function scrollLoop() {
            if (!isHovered) {
                // Scroll the container by a fixed distance to the left
                container.scrollLeft += scrollDistance;

                // If the container has scrolled past the total width of the content, reset the scroll position to 0
                if (container.scrollLeft >= container.scrollWidth / 2) {
                    container.scrollLeft = 0; // Reset to the first set of cards
                }
            }
        }

        // Function to start the scrolling loop
        function startScrolling() {
            scrollInterval = setInterval(scrollLoop, scrollSpeedInMs);
        }

        // Function to stop the scrolling loop
        function stopScrolling() {
            clearInterval(scrollInterval);
        }

        // Duplicate the cards inside the wrapper to create the infinite scroll illusion
        const cards = cardWrapper.innerHTML; // Get the original cards' HTML
        cardWrapper.innerHTML += cards; // Append the same cards to create the loop effect

