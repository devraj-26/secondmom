                                <script>
                                    function updateFileName(input) {
                                        const fileName = input.files[0]?.name || "Choose File"; // Get the name of the selected file
                                        const span = input.nextElementSibling; // The span element to update
                                        span.textContent = fileName; // Update the span with the file name
                                    }
