        <section style="margin-left: 100px; margin-top: 50px;">
            <div class="container" class="row">
                <div class="row justify-content-center">
                    <div class="col-12 col-md-10">
                        <!-- Title: Part of the form -->
                        <!-- <h5 class="mt-7 mb-4 pb-2  text-center col-lg-10" >Submit Your Details</h5> -->
                        <div class="col text-center" style="margin-left: -160px;">
                            <h3 class="ds-3 mt-3 mb-3" style="color: #7050f2;">Join Our Team</h3>
                            <span class="fs-5 fw-medium">Career opportunities that foster growth, challenge, and inspiration.
                                </span>
                        </div>
                        <!-- Form Start -->
                        <form id="career-form" method="POST" enctype="multipart/form-data">
                            <input type="hidden" name="formType" value="career-enquiry">

                            <div class="row mt-5 wow img-custom-anim-left">
                                <!-- Name Field -->
                                <div class="col-md-6 col-lg-5">
                                    <div class="input-group d-flex align-items-center">
                                        <div class="icon-input border border-end-0 rounded-2 rounded-end-0 ps-3">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                                <path class="stroke-dark" d="M12 11.25C13.7949 11.25 15.25 9.79493 15.25 8C15.25 6.20507 13.7949 4.75 12 4.75C10.2051 4.75 8.75 6.20507 8.75 8C8.75 9.79493 10.2051 11.25 12 11.25Z" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                                <path class="stroke-dark" d="M6.84723 19.25H17.1522C18.2941 19.25 19.1737 18.2681 18.6405 17.2584C17.856 15.7731 16.0677 14 11.9997 14C7.93174 14 6.1434 15.7731 5.35897 17.2584C4.8257 18.2681 5.70531 19.25 6.84723 19.25Z" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                            </svg>
                                        </div>
                                        <input type="text" class="form-control ms-0 border rounded-2 rounded-start-0 border-start-0" 
                                               name="<%= CAREER_ENQUIRY_DYNAMIC_FIELDS_KEYS.NAME %>" placeholder="Name *" 
                                               aria-label="username" id="name-input" required>
                                    </div>
                                    <small id="name-error" style="color: red; display: none;">Only alphabets are allowed.</small>
                                </div>
                                
                                <script>
                                    document.getElementById("name-input").addEventListener("input", function () {
                                        const inputField = this;
                                        const errorMessage = document.getElementById("name-error");
                                
                                        // Regex to allow only alphabets and spaces (no numbers or special characters)
                                        const namePattern = /^[A-Za-z\s]+$/;
                                
                                        if (namePattern.test(inputField.value) || inputField.value === "") {
                                            errorMessage.style.display = "none"; // Hide error message
                                        } else {
                                            errorMessage.style.display = "block"; // Show error message
                                            inputField.value = inputField.value.replace(/[^A-Za-z\s]/g, ""); // Remove invalid characters
                                        }
                                    });
                                </script>
                                
                                <!-- Contact Number Field -->
                                <div class="col-md-6 col-lg-5 mt-3 mt-md-0">
                                    <div class="input-group d-flex align-items-center mt-4 mt-md-0">
                                        <div class="icon-input border border-end-0 rounded-2 rounded-end-0 ps-3">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                                <path class="stroke-dark" d="M8.89286 4.75H6.06818C5.34017 4.75 4.75 5.34017 4.75 6.06818C4.75 13.3483 10.6517 19.25 17.9318 19.25C18.6598 19.25 19.25 18.6598 19.25 17.9318V15.1071L16.1429 13.0357L14.5317 14.6468C14.2519 14.9267 13.8337 15.0137 13.4821 14.8321C12.8858 14.524 11.9181 13.9452 10.9643 13.0357C9.98768 12.1045 9.41548 11.1011 9.12829 10.494C8.96734 10.1537 9.06052 9.76091 9.32669 9.49474L10.9643 7.85714L8.89286 4.75Z" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                            </svg>
                                        </div>
                                        <input type="tel" class="form-control ms-0 border rounded-2 rounded-start-0 border-start-0" 
                                               name="<%= CAREER_ENQUIRY_DYNAMIC_FIELDS_KEYS.CONTACT_NUMBER %>" 
                                               placeholder="Phone Number *" 
                                               aria-label="contact" 
                                               id="phone-input" 
                                               required maxlength="10">
                                    </div>
                                    <small id="phone-error" style="color: red; display: none;">Enter a valid 10-digit number.</small>
                                </div>
                                
                                <script>
                                    document.getElementById("phone-input").addEventListener("input", function () {
                                        const inputField = this;
                                        const errorMessage = document.getElementById("phone-error");
                                
                                        // Remove non-numeric characters
                                        inputField.value = inputField.value.replace(/\D/g, "");
                                
                                        // Check length
                                        if (inputField.value.length === 10) {
                                            errorMessage.style.display = "none"; // Hide error if valid
                                        } else {
                                            errorMessage.style.display = "block"; // Show error message
                                        }
                                    });
                                </script>
                                
                            </div>
            
                            <div class="row mt-3">
                                <!-- Email Field -->
                                <div class="col-md-8 col-lg-5">
                                    <div class="input-group d-flex align-items-center mt-4 mt-md-0">
                                        <div class="icon-input border border-end-0 rounded-2 rounded-end-0 ps-3">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                                <path class="stroke-dark" d="M3 5.75H21C21.6904 5.75 22.25 6.30964 22.25 7V17C22.25 17.6904 21.6904 18.25 21 18.25H3C2.30964 18.25 1.75 17.6904 1.75 17V7C1.75 6.30964 2.30964 5.75 3 5.75Z" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                                <path class="stroke-dark" d="M22 6.5L12 12.5L2 6.5" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                                            </svg>
                                        </div>
                                        <input type="email" class="form-control ms-0 border rounded-2 rounded-start-0 border-start-0" 
                                               name="<%= CAREER_ENQUIRY_DYNAMIC_FIELDS_KEYS.EMAIL %>" 
                                               placeholder="Email *" 
                                               aria-label="Email" 
                                               id="email-input" 
                                               required>
                                    </div>
                                    <small id="email-error" style="color: red; display: none;">Enter a valid email address.</small>
                                </div>
                                
                                <script>
                                    document.getElementById("email-input").addEventListener("input", function () {
                                        const emailField = this;
                                        const errorMessage = document.getElementById("email-error");
                                
                                        // Regex for email validation
                                        const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                                
                                        if (emailRegex.test(emailField.value)) {
                                            errorMessage.style.display = "none"; // Hide error if valid
                                        } else {
                                            errorMessage.style.display = "block"; // Show error message
                                        }
                                    });
                                </script>
                                

